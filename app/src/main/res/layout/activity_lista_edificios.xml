<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="itesm.mx.accesibilidad.ListaEdificios"
    android:clickable="false">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/edificio"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="106dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/button9"
        android:background="@drawable/rectoria"
        android:layout_below="@+id/button11"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/button10"
        android:background="@drawable/aulas1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/button11"
        android:background="@drawable/ciap"
        android:layout_below="@+id/button12"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/button12"
        android:background="@drawable/aulas4"
        android:layout_below="@+id/button10"
        android:layout_alignParentStart="true" />
</RelativeLayout>
